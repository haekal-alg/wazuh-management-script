  </rule>
  
  <rule id="950003" level="15">
    <if_group>sysmon_event1</if_group>
    
    <field name="win.eventdata.company" nocase="1">SecurityXploded</field>
    <field name="win.eventdata.image" type="pcre2" nocase="1">PasswordDump\.exe$</field>
    <field name="win.eventdata.originalFileName" type="pcre2" nocase="1">PasswordDump\.exe$</field>
    
    <description>Potential Hacktool 'SecurityXploded' Execution Detected. (Sigma ID: 7679d464-4f74-45e2-9e01-ac66c5eb041a)</description>
    <info type="link">https://securityxploded.com/</info>
